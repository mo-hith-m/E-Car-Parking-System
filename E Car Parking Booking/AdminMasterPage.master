                        <StaticHoverStyle BackColor="#FFCC00" BorderStyle="Inset" Font-Bold="True" 
                            ForeColor="Black" />
                        <StaticMenuItemStyle BackColor="Black" BorderStyle="Outset" Font-Bold="True" 
                            ForeColor="#FFCC00" HorizontalPadding="30px" />
