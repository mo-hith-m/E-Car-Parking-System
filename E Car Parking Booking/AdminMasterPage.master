    <style type="text/css">
        .style1
        {
            width: 90%;
        }
        .style2
        {
            height: 57px;
        }
        .style3
        {
            color: #FFFFFF;
        }
    </style>
</head>
<body>
    <form id="form1" runat="server">
    <div>
        <table align="center" class="style1">
            <tr>
                <td style="border: thin inset #6600CC; background-image: url('/E Car Parking Booking/background/btblueblk.gif');">
